<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main_interface"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    android:background="#E5E8E8"
    tools:context="com.example.autobot.quicksolutionfinal.MainInterface"
    android:weightSum="1">


    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@mipmap/ic_launcher"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/imageView2"
        android:layout_alignParentTop="true" />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="60dp"
        android:id="@+id/BtnSignOut"
        android:text="Sign Out"
        android:textSize="18dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="60dp"
        android:id="@+id/BtnCalculator"
        android:drawableLeft="@drawable/ic_dialpad_black_24dp"
        android:text="Calculator"
        android:layout_marginTop="-11dp"
        android:textSize="18sp"
        android:layout_below="@+id/BtnUpload"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />




    <Button
        android:layout_width="fill_parent"
        android:layout_height="60dp"
        android:id="@+id/BtnHome"
        android:drawableLeft="@drawable/ic_home_black_24dp"
        android:text="Home"
        android:textSize="18sp"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/imageView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="60dp"
        android:id="@+id/BtnUpload"
        android:drawableLeft="@drawable/ic_add_a_photo_black_24dp"
        android:text="Upload"
        android:textSize="18sp"
        android:layout_marginTop="49dp"
        android:layout_alignTop="@+id/BtnHome"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:id="@+id/textViewqs"
        android:text="Quick Solution"
        android:textSize="20sp"
        android:layout_above="@+id/BtnHome"
        android:layout_toLeftOf="@+id/imageView2"
        android:layout_toStartOf="@+id/imageView2"
        android:layout_marginBottom="17dp" />


</RelativeLayout>
